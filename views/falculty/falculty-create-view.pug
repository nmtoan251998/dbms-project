extends ../layout/common

block main
    link(rel="stylesheet", href="/styles/falculty-create.css")
    .container
        form.form(method="get")
            legend Faculty Information
            table
                tr
                    td
                        label(for="falid") Faculty's Id: 
                    td
                        input(type="text", name="falid")
                tr
                    td
                        label(for="falname") Faculty's Name: 
                    td
                        input(type="text", name="falname")
                tr
                    td 
                        label(for="dean") Faculty Dean: 
                    td
                        input(type="text", name="dean")
            button(type="submit") Submit
